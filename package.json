{
  "name": "atticus-social",
  "version": "1.0.0",
  "description": "A social web application",
  "main": "server.js",
  "scripts": {
    "server": "nodemon --watch backend --exec babel-node backend/server.js",
    "client": "cd frontend && npm start",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "build": "rm -rf dist && babel backend -d dist",
    "heroku-postbuild": "npm run build && cd frontend && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtticusFinchIPH/atticus-social.git"
  },
  "author": "atticus finch IPH (TRAN Van Duc)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AtticusFinchIPH/atticus-social/issues"
  },
  "homepage": "https://github.com/AtticusFinchIPH/atticus-social#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cloudinary": "^1.23.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-validator": "^6.9.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.25",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^6.4.18"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "nodemon": "^2.0.4"
  },
  "engines": {
    "node": "12.4.0",
    "npm": "6.9.0"
  }
}
